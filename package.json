{
  "name": "ngx-linkifyjs",
  "description": "Angular V6 wrapper for linkifyjs -  library for finding links in plain text and converting them to HTML &lt;a&gt; tags via linkifyjs",
  "version": "1.0.0",
  "homepage": "https://github.com/anthonynahas/ngx-linkifyjs",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/anthonynahas/ngx-linkifyjs.git"
  },
  "license": "MIT",
  "keywords": [
    "ngx",
    "angular",
    "library",
    "linkifyjs",
    "links",
    "find-links",
    "autolink",
    "text",
    "url",
    "email",
    "hashtags",
    "mentionsm"
  ],
  "bugs": {
    "url": "https://github.com/anthonynahas/ngx-linkifyjs/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=AnthonyNahas/ngx-linkifyjs --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/common": "^6.1.9",
    "@angular/core": "^6.1.9",
    "linkifyjs": "^2.1.7"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/platform-server": "^6.1.9",
    "@compodoc/gulp-compodoc": "0.0.9",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "8.0.44",
    "angular-cli-ghpages": "^0.5.3",
    "angular2-template-loader": "0.6.2",
    "ansi-colors": "^3.1.0",
    "autoprefixer": "^9.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "^4.4.4",
    "commitplease": "3.2.0",
    "conventional-github-releaser": "2.0.2",
    "core-js": "2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "del": "3.0.0",
    "fancy-log": "1.3.2",
    "greenkeeper-lockfile": "1.15.1",
    "gulp": "3.9.1",
    "gulp-bump": "3.1.1",
    "gulp-conventional-changelog": "^2.0.3",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "2.7.0",
    "gulp-hub": "0.8.0",
    "gulp-inline-ng2-template": "5.0.1",
    "gulp-tslint": "8.1.3",
    "html-loader": "0.5.5",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-preset-angular": "5.2.3",
    "lodash": "4.17.10",
    "node-sass": "^4.9.3",
    "postcss": "^7.0.4",
    "postcss-strip-inline-comments": "0.1.5",
    "pump": "3.0.0",
    "raw-loader": "0.5.1",
    "rollup": "^0.66.3",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.1",
    "rxjs": "^6.3.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "to-string-loader": "1.1.5",
    "travis-status": "^3.0.1",
    "tslint": "~5.11.0",
    "typescript": "2.7.2",
    "yargs": "12.0.1",
    "zone.js": "0.8.26"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "linkifyjs",
      "@types/jasmine",
      "@types/jest",
      "@types/node",
      "conventional-github-releaser",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  }
}
