{
  "name": "ngx-linkifyjs",
  "description": "Angular V7 wrapper for linkifyjs -  library for finding links in plain text and converting them to HTML &lt;a&gt; tags via linkifyjs",
  "version": "1.3.0",
  "homepage": "https://github.com/anthonynahas/ngx-linkifyjs",
  "author": {
    "name": "Anthony Nahas",
    "url": "https://github.com/anthonynahas"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/anthonynahas/ngx-linkifyjs.git"
  },
  "license": "MIT",
  "schematics": "./schematics/collection.json",
  "keywords": [
    "ngx",
    "angular",
    "library",
    "linkifyjs",
    "links",
    "find-links",
    "autolink",
    "text",
    "url",
    "email",
    "hashtags",
    "mentionsm"
  ],
  "bugs": {
    "url": "https://github.com/anthonynahas/ngx-linkifyjs/issues"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=AnthonyNahas/ngx-linkifyjs --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/common": "^7.2.9",
    "@angular/core": "^7.2.9",
    "linkifyjs": "^2.1.8"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^7.3.6",
