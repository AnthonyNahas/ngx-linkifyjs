    "@angular/platform-server": "^7.2.9",
    "@compodoc/gulp-compodoc": "^0.0.10",
    "@types/jasmine": "^3.3.10",
    "@types/jest": "^23.3.14",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.14.1",
    "angular-cli-ghpages": "^0.5.3",
